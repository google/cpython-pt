else -> senao_
while -> enquanto_
for -> para_
in -> em_
try -> tentar_
finally -> finalmente_
with -> com_
except -> exceto_
lambda -> lambda_
or -> ou_
and -> e_
not -> nao_
is -> eh_
class -> classe_
for -> para_
yield -> ceder_


